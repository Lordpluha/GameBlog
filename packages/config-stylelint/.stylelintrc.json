{
	"extends": "stylelint-config-standard-scss",
	"rules": {
		"scss/at-rule-no-unknown": [
			true,
			{
				"ignoreAtRules": [
					"tailwind",
					"apply",
					"variants",
					"responsive",
					"screen"
				]
			}
		],
		"declaration-block-trailing-semicolon": "never",
		"no-descending-specificity": null
	}
}
